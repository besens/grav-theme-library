<footer id="footer">
    <div class="uk-container uk-container-center uk-vertical-align-top">
        <div class="uk-grid">
            <div class="uk-width-1-4">



            </div>
            <div class="uk-width-1-4">



            </div>
            <div class="uk-width-1-4">



            </div>
            <div class="uk-width-1-4">
                <ul class="uk-list uk-list-space">
                    {% for link in site.quicklinks %}
                        <li><a href="{{ link.url }}">{{ link.title }}</a>
                        </li>
                    {% endfor %}
                </ul>
            </div>
        </div>
        <aside class="clients">
            <div class="uk-flex uk-flex-space-between">
                {% for client in config.site.clients %}
                    <div class="uk-small-1-1 uk-medium-1-2 uk-large 1-3">
                        <a href="{{ client.url }}">
                            <img src="{{ theme_url }}/img/{{ client.logo }}" alt="">
                        </a>
                    </div>
                {% endfor %} 
            </div>
        </aside>
    </div>
    <div class="uk-width-1-1">
        <nav class="uk-navbar uk-navbar-attached">
            <div class="uk-container uk-container-center">
                <div class="uk-navbar-content">
                   
                </div>
     
                <div class="uk-navbar-flip">
                    {% for network in site.social %}
                        <a href="{{ network.url }}" class="uk-icon-button uk-icon-hover uk-text-primary"><i class="uk-icon-{{ network.title }}"></i></a>
                    {% endfor %}

                </div>

                <div class="uk-navbar-content uk-navbar-center">
                    <a href="#" id="toTop" class="uk-icon-button uk-icon-hover" data-uk-smooth-scroll><i class="uk-icon-arrow-up"></i></a>
                </div>


             </div>
        </nav> 

    </div>


</footer>